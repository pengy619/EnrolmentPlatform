<script>
    var table, layer, laydate, form;
    layui.use(['jquery', 'table', 'laydate', 'layer'], function () {
        table = layui.table;
        layer = layui.layer;
        laydate = layui.laydate;
        form = layui.form;

        //日期
        laydate.render({
            elem: '#Date'
            , range: true
        });

        //实例表格
        var tableOptions = {
            elem: '#table'
            , skin: 'line'
            , limit: 10
            , url: '@Url.Action("Search", "Manager")'
            , page: true //开启分页
            , cols: [[ //表头
                { type: 'checkbox' }
                , { field: 'StudentName', title: '学生姓名', width: 100 }
                , { field: 'IDCardNo', title: '身份证号', width: 100 }
                , { field: 'BatchName', title: '批次', width: 80 }
                , { field: 'SchoolName', title: '院校', width: 150 }
                , { field: 'LevelName', title: '层次', width: 100 }
                , { field: 'MajorName', title: '专业', width: 180 }
                , { title: '电子照', toolbar: '#tableToolImage', width: 100 }
                , { field: 'FromChannelName', title: '来源机构', width: 120 }
                , { field: 'ToLearningCenterName', title: '学院中心', width: 120 }
                , { field: 'StatusName', title: '状态', width: 80 }
                , { field: 'XueHao', title: '学号', width: 120 }
                , { field: 'CreateTimeStr', title: '报名时间', width: 120 }
                , { field: 'CreateUserName', title: '招生老师', width: 90 }
                , { title: '操作', toolbar: '#tableTool', width: 168 }
            ]]
        }
        table.render(tableOptions);

        //删除
        form.on('submit(d)', function (data) {
            //选中行
            var checkdata = table.checkStatus('table').data;
            if (checkdata.length == 0) {
                layer.msg('请选择需要删除的报名单。', {
                    icon: 2,
                    time: 2000
                });
                return false;
            }

            //删除处理
            var canDelete = true;
            var arr = new Array(checkdata.length);
            for (var i = 0; i < checkdata.length; i++) {
                arr[i] = checkdata[i].OrderId;
                if (parseInt(checkdata[i].Status) != 0 && parseInt(checkdata[i].Status) != 3) {
                    canDelete = false;
                }
            }

            if (canDelete == false) {
                layer.msg('只有草稿和拒绝状态的才可以删除。', {
                    icon: 2,
                    time: 2000
                });
                return false;
            }

            //删除确认
            layer.confirm('确认删除？', {
                btn: ['确认', '取消']
            }, function (index, layero) {
                $.ajax({
                    type: "POST",
                    contentType: "application/json;utf-8",
                    dataType: "json",
                    async: false,
                    data: JSON.stringify(arr),
                    url: '@Url.Action("Delete", "Manager")',
                    success: function (result) {
                        if (result.ret == 1) {
                            layer.msg('删除成功。', {
                                icon: 1,
                                time: 2000
                            }, function () {
                                table.reload("table");
                            });
                        }
                        else {
                            layer.msg(result.msg, {
                                icon: 2,
                                time: 2000
                            });
                        }
                    }
                });

                layer.close(index);
            });
        });

        //查询
        form.on('submit(s)', function (data) {
            //执行重载
            table.reload('table', {
                page: {
                    curr: 1 //重新从第 1 页开始
                }
                , where: getSearchData()
            });
        });

        //毕业
        form.on('submit(by)', function (data) {
            //选中行
            var checkdata = table.checkStatus('table').data;
            if (checkdata.length == 0) {
                layer.msg('请选择需要毕业的报名单。', {
                    icon: 2,
                    time: 2000
                });
                return false;
            }

            //毕业处理
            var canBiYe = true;
            var arr = new Array(checkdata.length);
            for (var i = 0; i < checkdata.length; i++) {
                arr[i] = checkdata[i].OrderId;
                if (parseInt(checkdata[i].Status) != 7) {
                    canBiYe = false;
                }
            }

            if (canBiYe == false) {
                layer.msg('只有已录取状态的才可以毕业。', {
                    icon: 2,
                    time: 2000
                });
                return false;
            }

            //毕业确认
            layer.confirm('确认毕业？', {
                btn: ['确认', '取消']
            }, function (index, layero) {
                $.ajax({
                    type: "POST",
                    contentType: "application/json;utf-8",
                    dataType: "json",
                    async: false,
                    data: JSON.stringify(arr),
                    url: '@Url.Action("Graduated", "Manager")',
                    success: function (result) {
                        if (result.ret == 1) {
                            layer.msg('毕业操作成功。', {
                                icon: 1,
                                time: 2000
                            }, function () {
                                table.reload("table");
                            });
                        }
                        else {
                            layer.msg(result.msg, {
                                icon: 2,
                                time: 2000
                            });
                        }
                    }
                });

                layer.close(index);
            });
        });

        //选择学院中心
        form.on('submit(bs)', function (data) {
            var ret = checkBaosong();
            if (ret.result == false) {
                return false;
            }

            layerIndex = layer.open({
                type: 1,
                title: "选择学院中心",
                area: ["450px", "200px"],
                content: $('#audit-btn-modal2'),
                yes: function (index, layero) {
                    //事件
                    layer.close(indxex);
                }
            });
        });

        //报送校验
        function checkBaosong() {
            var ret = {};
            //选中行
            var checkdata = table.checkStatus('table').data;
            if (checkdata.length == 0) {
                layer.msg('请选择需要报送的报名单。', {
                    icon: 2,
                    time: 2000
                });
                ret.result = false;
                return ret;
            }

            //选中的ID
            var canSubmit = true;
            var arr = new Array(checkdata.length);
            for (var i = 0; i < checkdata.length; i++) {
                arr[i] = checkdata[i].OrderId;
                //已报名，草稿，被拒绝可以进行报送
                if (parseInt(checkdata[i].Status) != 4 && parseInt(checkdata[i].Status) != 0 && parseInt(checkdata[i].Status) != 3) {
                    canSubmit = false;
                }
            }

            if (canSubmit == false) {
                layer.msg('存在不能进行报送的数据。', {
                    icon: 2,
                    time: 2000
                });
                ret.result = false;
                return ret;
            }

            ret.result = true;
            ret.arr = arr;
            return ret;
        }

        var layerIndex = 1;
        //报送学院中心
        form.on('submit(bsc)', function (data) {
            var ret = checkBaosong();
            if (ret.result == false) {
                return false;
            }

            var dto = {};
            dto.ids = ret.arr;
            dto.learningCenterId = $("#LearningCenter").val();
            if (dto.learningCenterId == false) {
                layer.msg('请选择学院中心！', {
                    icon: 2,
                    time: 2000
                });
                return false;
            }
            $.ajax({
                type: "POST",
                contentType: "application/json;utf-8",
                dataType: "json",
                async: false,
                data: JSON.stringify(dto),
                url: '@Url.Action("ToLearningCenter", "Manager")',
                success: function (result) {
                    if (result.ret == 1) {
                        layer.msg('报送成功。', {
                            icon: 1,
                            time: 2000
                        }, function () {
                            layer.close(layerIndex);
                            table.reload("table");
                        });
                    }
                    else {
                        layer.msg(result.msg, {
                            icon: 2,
                            time: 2000
                        });
                    }
                }
            });
        });

        //渠道修改
        var qudaoxiugaiIndex = 0;
        form.on('submit(xgqd)', function (data) {

            //选中行
            var checkdata = table.checkStatus('table').data;
            if (checkdata.length == 0) {
                layer.msg('请选择需要修改来源的报名单。', {
                    icon: 2,
                    time: 2000
                });
                return false;
            }

            qudaoxiugaiIndex = layer.open({
                type: 1,
                title: "选择来源机构",
                area: ["450px", "200px"],
                content: $('#audit-btn-modalqd'),
                yes: function (index, layero) {
                    //事件
                    layer.close(indxex);
                }
            });
        });

        //渠道修改确认
        form.on('submit(qdxgConfirm)', function (data) {
            //选中行
            var checkdata = table.checkStatus('table').data;
            if (checkdata.length == 0) {
                layer.msg('请选择需要修改来源的报名单。', {
                    icon: 2,
                    time: 2000
                });
                return false;
            }

            var arr = new Array(checkdata.length);
            for (var i = 0; i < checkdata.length; i++) {
                arr[i] = checkdata[i].OrderId;
            }

            var dto = {};
            dto.ids = arr;
            dto.trainingInstitutionsId = $("#trainingModify").val();
            $.ajax({
                type: "POST",
                contentType: "application/json;utf-8",
                dataType: "json",
                async: false,
                data: JSON.stringify(dto),
                url: '@Url.Action("UpdateTrainingInstitutions", "Manager")',
                success: function (result) {
                    if (result.ret == 1) {
                        layer.msg('修改成功。', {
                            icon: 1,
                            time: 2000
                        }, function () {
                            layer.close(qudaoxiugaiIndex);
                            table.reload("table");
                        });
                    }
                    else {
                        layer.msg(result.msg, {
                            icon: 2,
                            time: 2000
                        });
                    }
                }
            });
        });

        //拒绝弹窗
        form.on('submit(reject)', function (data) {
            var ret = checkReject();
            if (ret.result == false) {
                return false;
            }

            layerIndex2 = layer.open({
                type: 1,
                title: "拒绝确认",
                area: ["420px", "250px"],
                content: $('#audit-btn-modal3'),
                yes: function (index, layero) {
                    //事件
                    layer.close(indxex);
                }
            });
        });

        //拒绝校验
        function checkReject() {
            var ret = {};
            //选中行
            var checkdata = table.checkStatus('table').data;
            if (checkdata.length == 0) {
                layer.msg('请选择需要拒绝的报名单。', {
                    icon: 2,
                    time: 2000
                });
                ret.result = false;
                return ret;
            }

            //选中的ID
            var canSubmit = true;
            var arr = new Array(checkdata.length);
            for (var i = 0; i < checkdata.length; i++) {
                arr[i] = checkdata[i].OrderId;
                //已报名可以进行拒绝
                if (parseInt(checkdata[i].Status) != 4) {
                    canSubmit = false;
                }
            }

            if (canSubmit == false) {
                layer.msg('只有已报名的能进行拒绝。', {
                    icon: 2,
                    time: 2000
                });
                ret.result = false;
                return ret;
            }

            ret.result = true;
            ret.arr = arr;
            return ret;
        }

        var layerIndex2 = 2;
        //拒绝确认
        form.on('submit(rejectc)', function (data) {
            var ret = checkReject();
            if (ret.result == false) {
                return false;
            }

            var dto = {};
            dto.ids = ret.arr;
            dto.reason = $.trim($("#rejectReason").val());
            if (dto.reason == "") {
                layer.msg('请输入拒绝理由！', {
                    icon: 2,
                    time: 2000
                });
                return false;
            }

            $.ajax({
                type: "POST",
                contentType: "application/json;utf-8",
                dataType: "json",
                async: false,
                data: JSON.stringify(dto),
                url: '@Url.Action("Reject", "Manager")',
                success: function (result) {
                    if (result.ret == 1) {
                        layer.msg('拒绝成功。', {
                            icon: 1,
                            time: 2000
                        }, function () {
                            layer.close(layerIndex2);
                            table.reload("table");
                        });
                    }
                    else {
                        layer.msg(result.msg, {
                            icon: 2,
                            time: 2000
                        });
                    }
                }
            });
        });

        //报名单导入
        var orderuploadIndex = 0;
        form.on('submit(upload)', function (data) {
            orderuploadIndex = layer.open({
                type: 1,
                title: "导入报名单",
                area: ["450px", "200px"],
                content: $('#audit-btn-modalupload'),
                yes: function (index, layero) {
                    //事件
                    layer.close(indxex);
                }
            });
