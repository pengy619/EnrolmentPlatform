<script>
    var table, layer, laydate, form;
    layui.use(['jquery', 'table', 'laydate', 'layer'], function () {
        table = layui.table;
        layer = layui.layer;
        laydate = layui.laydate;
        form = layui.form;
        form.render();

        //日期
        laydate.render({
            elem: '#Date'
            , range: true
        });

        //实例表格
        var tableOptions = {
            elem: '#table'
            , skin: 'line'
            , limit: 10
            , url: '@Url.Action("Search", "Manager")'
            , page: true //开启分页
            , cols: [[ //表头
                { type: 'checkbox' }
                , { field: '', title: '学生头像', align: 'center', templet: '#stuInfoImg' }
                , { field: '', title: '学员信息', align: 'center', templet: '#stuInfoTpl' }
                , { field: '', title: '报读信息', align: 'center', templet: '#stuForReadTpl' }
                , { field: '', title: '状态', align: 'center', width: 150, templet: '#statusTpl' }
                , { field: '', title: '操作', align: 'center', fixed: 'right', templet: '#tableTool', width: 300 }
            ]]
            , done: function () {
                $.ajax({
                    url: "@Url.Action("GetOrderStatistics", "Manager")",
                    data: getSearchData(),
                    type: "post",
                    dataType: "json",
                    success: function (data) {
                        $("#countAll").html(data.countAll);
                        $("#count0").html(data.count0);
                        $("#count2").html(data.count2);
                        $("#count3").html(data.count3);
                        $("#count4").html(data.count4);
                        $("#count5").html(data.count5);
                        $("#count6").html(data.count6);
                        $("#count7").html(data.count7);
                        $("#count8").html(data.count8);
                        $("#count9").html(data.count9);
                    }
                })
            }
        }
        table.render(tableOptions);

        //监听工具条
        table.on('tool(table)', function (obj) {
            var data = obj.data;
            if (obj.event === 'status') {
                layer.open({
                    type: 1,
                    title: '修改订单状态',
                    btn: ['确定', '取消'],
                    area: ["400px", "200px"],
                    content: $('#btn-modal'),
                    yes: function () {
                        var status = $("#orderStatus").val();
                        $.ajax({
                            url: "@Url.Action("UpdateOrderStatus", "Manager")",
                            type: "post",
                            data: { orderId: data.OrderId, status: status },
                            dataType: "json",
                            success: function (result) {
                                if (result.ret == 1) {
                                    layer.msg("保存成功", {
                                        icon: 1
                                        , time: 1000
                                    }, function () {
                                        table.reload("table");
                                        layer.closeAll();
                                    });
                                } else {
                                    layer.msg(result.msg, {
                                        icon: 2
                                        , time: 2000
                                    });
                                }
                            }
                        });
                    },
                    btn2: function () {
                    }
                });
            }
        });

        //删除
        form.on('submit(d)', function (data) {
            //选中行
            var checkdata = table.checkStatus('table').data;
            if (checkdata.length == 0) {
                layer.msg('请选择需要删除的报名单。', {
                    icon: 2,
                    time: 2000
                });
                return false;
            }

            //删除处理
            var canDelete = true;
            var arr = new Array(checkdata.length);
            for (var i = 0; i < checkdata.length; i++) {
                arr[i] = checkdata[i].OrderId;
                if (parseInt(checkdata[i].Status) != 0 && parseInt(checkdata[i].Status) != 3) {
                    canDelete = false;
                }
            }

            if (canDelete == false) {
                layer.msg('只有预报名和审核不通过的才可以删除。', {
                    icon: 2,
                    time: 2000
                });
                return false;
            }

            //删除确认
            layer.confirm('确认删除？', {
                btn: ['确认', '取消']
            }, function (index, layero) {
                $.ajax({
                    type: "POST",
                    contentType: "application/json;utf-8",
                    dataType: "json",
                    async: false,
                    data: JSON.stringify(arr),
                    url: '@Url.Action("Delete", "Manager")',
                    success: function (result) {
                        if (result.ret == 1) {
                            layer.msg('删除成功。', {
                                icon: 1,
                                time: 2000
                            }, function () {
                                table.reload("table");
                            });
                        }
                        else {
                            layer.msg(result.msg, {
                                icon: 2,
                                time: 2000
                            });
                        }
                    }
                });

                layer.close(index);
            });
        });

        //查询
        form.on('submit(s)', function (data) {
            //执行重载
            table.reload('table', {
                page: {
                    curr: 1 //重新从第 1 页开始
                }
                , where: getSearchData()
            });
        });

        //毕业
        form.on('submit(by)', function (data) {
            //选中行
            var checkdata = table.checkStatus('table').data;
            if (checkdata.length == 0) {
                layer.msg('请选择需要毕业的报名单。', {
                    icon: 2,
                    time: 2000
                });
                return false;
            }

            //毕业处理
            var canBiYe = true;
            var arr = new Array(checkdata.length);
            for (var i = 0; i < checkdata.length; i++) {
                arr[i] = checkdata[i].OrderId;
                if (parseInt(checkdata[i].Status) != 7) {
                    canBiYe = false;
                }
            }

