@{
    ViewBag.Title = "报名单列表";
    Layout = "~/Views/Shared/_Layout.cshtml";
    List<EnrolmentPlatform.Project.DTO.Accounts.SupplierListDto> trainingList = ViewBag.TrainingList;
    List<EnrolmentPlatform.Project.DTO.Accounts.SupplierListDto> learningList = ViewBag.LearningList;
    List<EnrolmentPlatform.Project.DTO.Basics.MetadataDto> schoolList = ViewBag.SchoolList;
}
<style type="text/css">
    .searchMore {
        display: none;
    }

    #audit-btn-modal2 .layui-form-select dl {
        max-height: 84px;
    }

    .layui-table-body {
        overflow-x: auto;
    }

    .layui-form-radio {
        margin: 6px 0px 0 0;
    }

    .layui-table-cell {
        height: auto;
    }

    .text-left {
        text-align: left;
    }

    .layui-layer-page .layui-layer-content {
        height: 80px !important;
        overflow: visible;
    }
</style>
<div class="main-wrap">
    <div class="panel">
        <div class="panel-title">
            <div class="layui-form">
                <input type="hidden" name="Status" id="Status" value="">
                <div class="layui-form-item flex">
                    <label class="layui-form-label">快速查询</label>
                    <div class="filter-list">
                        <span class="count_type layui-btn layui-btn-sm layui-btn-gray actived" onclick="toChang('', this)">全部（<span id="countAll">0</span>）</span>
                        <span class="count_type layui-btn layui-btn-sm layui-btn-gray" onclick="toChang('0', this)">预报名（<span id="count0">0</span>）</span>
                        <span class="count_type layui-btn layui-btn-sm layui-btn-gray" onclick="toChang('4', this)">已报名（<span id="count4">0</span>）</span>
                        <span class="count_type layui-btn layui-btn-sm layui-btn-gray" onclick="toChang('5', this)">已报送（<span id="count5">0</span>）</span>
                        <span class="count_type layui-btn layui-btn-sm layui-btn-gray" onclick="toChang('6', this)">已初审（<span id="count6">0</span>）</span>
                        <span class="count_type layui-btn layui-btn-sm layui-btn-gray" onclick="toChang('3', this)">审核不通过（<span id="count3">0</span>）</span>
                        <span class="count_type layui-btn layui-btn-sm layui-btn-gray" onclick="toChang('7', this)">已录取（<span id="count7">0</span>）</span>
                        <span class="count_type layui-btn layui-btn-sm layui-btn-gray" onclick="toChang('8', this)">已毕业（<span id="count8">0</span>）</span>
                        <span class="count_type layui-btn layui-btn-sm layui-btn-gray" onclick="toChang('9', this)">已休学（<span id="count9">0</span>）</span>
                        <span class="count_type layui-btn layui-btn-sm layui-btn-gray" onclick="toChang('2', this)">已退学（<span id="count2">0</span>）</span>
                    </div>
                </div>
                <div class="layui-form-item flex">
                    <label class="layui-form-label">学生姓名</label>
                    <div class="layui-input-inline">
                        <input type="text" id="StudentName" class="layui-input">
                    </div>
                    <button class="layui-btn layui-btn-warm" type="button" id="search_btn" lay-submit="" lay-filter="s">查询</button>
                    <button class="layui-btn layui-btn-warm" type="button" id="MoreSearch">更多查询</button>
                </div>
                <div class="layui-form-item flex searchMore">
                    <label class="layui-form-label">学生电话</label>
                    <div class="layui-input-inline">
                        <input type="text" id="Phone" class="layui-input">
                    </div>
                    <label class="layui-form-label">身份证号</label>
                    <div class="layui-input-inline">
                        <input type="text" id="IDCard" class="layui-input">
                    </div>
                </div>
                <div class="layui-form-item flex searchMore">
                    <label class="layui-form-label">招生老师</label>
                    <div class="layui-input-inline">
                        <input type="text" id="CreateUserName" class="layui-input">
                    </div>
                    <label class="layui-form-label">报考学校</label>
                    <div class="layui-input-inline">
                        <input type="text" id="SchoolName" class="layui-input">
                    </div>
                    <label class="layui-form-label">报考专业</label>
                    <div class="layui-input-inline">
                        <input type="text" id="MajorName" class="layui-input">
                    </div>
                </div>
                <div class="layui-form-item flex searchMore">
                    <label class="layui-form-label">报读层次</label>
                    <div class="layui-input-inline">
                        <input type="text" id="LevelName" class="layui-input">
                    </div>
                    <label class="layui-form-label">报名时间</label>
                    <div class="layui-input-inline">
                        <input type="text" id="Date" readonly="readonly" class="layui-input">
                    </div>
                </div>
                <div class="layui-form-item flex searchMore">
                    <label class="layui-form-label">来源（机构）</label>
                    <div class="layui-input-inline">
                        <select id="FromChannelId" name="FromChannelId" style="width:100px;" lay-search>
                            <option value="">全部</option>
                            @foreach (var item in trainingList)
                            {
                                <option value="@item.SupplierId">@item.SupplierName</option>
                            }
                        </select>
                    </div>
                    <label class="layui-form-label">去向（学院中心）</label>
                    <div class="layui-input-inline">
                        <select id="ToLearningCenterId" name="ToLearningCenterId" style="width:100px;" lay-search>
                            <option value="">全部</option>
                            @foreach (var item in learningList)
                            {
                                <option value="@item.SupplierId">@item.SupplierName</option>
                            }
                        </select>
                    </div>
                </div>
                <div class="layui-form-item flex searchMore">
                    <label class="layui-form-label">批次</label>
                    <div class="layui-input-inline">
                        <input type="text" id="BatchName" class="layui-input">
                    </div>
                    <label class="layui-form-label">照片全上传</label>
                    <div class="layui-input-inline">
                        <input type="radio" name="AllOrderImageUpload" value="" title="全部" checked>
                        <input type="radio" name="AllOrderImageUpload" value="true" title="是">
                        <input type="radio" name="AllOrderImageUpload" value="false" title="否">
                    </div>
                </div>
                @*<a href="@Url.Action("Option","Manager",new { area="Order"})" class="layui-btn ">新增</a>*@
                <button class="layui-btn layui-btn-normal layui-btn-small" lay-submit="" lay-filter="bs">报送</button>
                <button class="layui-btn layui-btn-normal layui-btn-small" lay-submit="" lay-filter="by">毕业</button>
                <button class="layui-btn layui-btn-warm" lay-submit="" lay-filter="xgqd">修改来源</button>
                <button class="layui-btn layui-btn-danger" lay-submit="" lay-filter="reject">拒绝</button>
                <button class="layui-btn layui-btn-danger" lay-submit="" lay-filter="re">退学</button>
                @*<button class="layui-btn layui-btn-danger" lay-submit="" lay-filter="d">删除</button>*@
                <button class="layui-btn layui-btn-danger" lay-submit="" lay-filter="upload">订单导入</button>
                <button class="layui-btn layui-btn-danger" lay-submit="" lay-filter="luquUpload">录取导入</button>
                <button class="layui-btn layui-btn-danger" lay-submit="" lay-filter="auditUpload">初审导入</button>
                <button class="layui-btn layui-btn-danger" lay-submit="" lay-filter="rejectUpload">审核不通过导入</button>
                <a href="javascript:exportOpreation();" id="export" class="layui-btn layui-btn-primary">导出</a>
                @*<a href="/Temp/student.xls" style=" float: right;margin-top: 5px;text-decoration: underline;color:blue;">导入模板下载</a>*@
                @*<a href="/Temp/luqutemp.xls" style=" float: right;margin-top: 5px;text-decoration: underline;color:blue;margin-right: 10px;">录取模板下载</a>*@
                @*<a href="/Temp/AuditUploadTemp.xls" style=" float: right;margin-top: 5px;text-decoration: underline;color:blue;margin-right: 10px;">初审模板下载</a>*@
            </div>
        </div>
        <div class="panel-body">
            <table id="table" lay-filter="table"></table>
        </div>
    </div>
</div>
<!-- 弹出层（操作日志） -->
<div id="audit-btn-modal" style="display:none;">
    <div class="layui-form big-label" style="width:795px; height:250px;overflow-y:auto;overflow-x:hidden;">
        <table class="layui-hide" id="Tb_LogSettingForTable"></table>
    </div>
</div>
<!-- 弹出层（修改订单状态） -->
<div id="btn-modal" style="display:none;">
    <div class="layui-form margin-top-20">
        <div class="layui-form-item big-label">
            <label class="layui-form-label">订单状态</label>
            <div class="layui-input-inline">
                <select id="orderStatus" name="orderStatus">
                    <option value="6">已初审</option>
                    <option value="7">已录取</option>
                    <option value="8">已毕业</option>
                    <option value="9">已休学</option>
                    <option value="2">已退学</option>
                </select>
            </div>
        </div>
    </div>
</div>
<!-- 弹出层（报送学院中心） -->
<div id="audit-btn-modal2" style="display:none;">
    <div class="layui-form">
        <div class="layui-form-item required" style="padding-top: 20px;">
            <label class="layui-form-label">学院中心</label>
            <div class="layui-input-inline">
                <select id="LearningCenter" name="LearningCenter" lay-search>
                    @if (learningList != null && learningList.Count > 0)
                    {
                        foreach (var item in learningList)
                        {
                            <option value="@item.SupplierId">@item.SupplierName</option>
                        }
                    }
                </select>
            </div>
            <button class="layui-btn layui-btn-warm" lay-submit="" lay-filter="bsc">确定</button>
        </div>
    </div>
</div>
<!-- 弹出层（拒绝确认） -->
<div id="audit-btn-modal3" style="display:none;">
    <div class="layui-form" style="text-align: center;">
        <div class="layui-form-item required" style="padding-top: 20px;">
            <label class="layui-form-label">拒绝理由</label>
            <div class="layui-input-inline">
                <textarea name="rejectReason" placeholder="请输入拒绝理由" style="width:250px;" id="rejectReason" class="layui-textarea"></textarea>
            </div>
        </div>
        <button class="layui-btn layui-btn-warm" lay-submit="" lay-filter="rejectc">确定</button>
    </div>
</div>
<!-- 修改来源渠道 -->
<div id="audit-btn-modalqd" style="display:none;">
    <div class="layui-form">
        <div class="layui-form-item required" style="padding-top: 20px;">
            <label class="layui-form-label">招生机构</label>
            <div class="layui-input-inline">
                <select id="trainingModify" name="trainingModify" lay-search>
                    @if (trainingList != null && trainingList.Count > 0)
                    {
                        foreach (var item in trainingList)
                        {
