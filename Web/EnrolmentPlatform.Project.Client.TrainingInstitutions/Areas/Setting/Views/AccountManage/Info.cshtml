@{
    ViewBag.Title = "账户信息";
    Layout = "~/Views/Shared/_Layout.cshtml";

    EnrolmentPlatform.Project.DTO.Accounts.SupplierEnterpriseGetDto dto = ViewBag.EntityData;
}

<div class="main-wrap">
    <div class="panel">
        <div class="panel-title">
            基础信息
        </div>
        <div class="panel-body">
            <div class="info-cell no-padding">
                <div class="row">
                    <div class="item">
                        <span class="label">招生机构</span>
                        <span class="text">@dto.EnterpriseName</span>
                    </div>
                </div>
                <div class="row">
                    <div class="item">
                        <span class="label">联系人</span>
                        <span class="text">@dto.Contact</span>
                    </div>
                </div>
                <div class="row">
                    <div class="item">
                        <span class="label">手机号</span>
                        <span class="text">@dto.Phone</span>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="panel">
        <div class="panel-title">
            账号信息
        </div>
        <div class="panel-body">
            <div class="info-cell no-padding">
                <div class="row">
                    <div class="item">
                        <span class="label">登录账号</span>
                        <span class="text">@dto.UserAccount</span>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div> 